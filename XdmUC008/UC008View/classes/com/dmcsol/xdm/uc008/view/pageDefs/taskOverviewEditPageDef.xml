<?xml version="1.0" encoding="UTF-8" ?>
<pageDefinition xmlns="http://xmlns.oracle.com/adfm/uimodel" version="12.1.2.66.68" id="taskOverviewEditPageDef"
                Package="com.dmcsol.xdm.uc008.view.pageDefs">
  <parameters/>
  <executables>
    <variableIterator id="variables">
      <variableUsage DataControl="XdmUC008ServiceDataControl" Binds="TaskVO1.variablesMap.pResponsible"
                     Name="ExecuteWithParams_pResponsible" IsQueriable="false"/>
      <variableUsage DataControl="XdmUC008ServiceDataControl" Binds="TaskVO1.variablesMap.pProgramme"
                     Name="ExecuteWithParams_pProgramme" IsQueriable="false"/>
      <variableUsage DataControl="XdmUC008ServiceDataControl" Binds="TaskVO1.variablesMap.pText"
                     Name="ExecuteWithParams_pText" IsQueriable="false"/>
    </variableIterator>
    <iterator Binds="TaskVO1" RangeSize="25" DataControl="XdmUC008ServiceDataControl" id="TaskVO1Iterator"/>
    <iterator Binds="ProgrammeManagerLOV1" RangeSize="-1" DataControl="XdmLovServiceDataControl"
              id="ProgrammeManagerLOV1Iterator"/>
    <iterator Binds="ProgrammeLOV1" RangeSize="-1" DataControl="XdmLovServiceDataControl" id="ProgrammeLOV1Iterator"/>
  </executables>
  <bindings>
    <tree IterBinding="TaskVO1Iterator" id="TaskVO1">
      <nodeDefinition DefName="com.dmcsol.xdm.uc008.model.view.TaskVO" Name="TaskVO10">
        <AttrNames>
          <Item Value="StartDate"/>
          <Item Value="Text"/>
          <Item Value="StartWhere"/>
          <Item Value="FlightNo"/>
          <Item Value="EndDate"/>
          <Item Value="EndWhere"/>
          <Item Value="Pax"/>
          <Item Value="ElemKey"/>
          <Item Value="Remarks"/>
        </AttrNames>
      </nodeDefinition>
    </tree>
    <action IterBinding="TaskVO1Iterator" id="ExecuteWithParams" RequiresUpdateModel="true" Action="executeWithParams">
      <NamedData NDName="pResponsible" NDType="java.lang.String" NDValue="${bindings.ExecuteWithParams_pResponsible}"/>
      <NamedData NDName="pProgramme" NDType="java.lang.String" NDValue="${bindings.ExecuteWithParams_pProgramme}"/>
      <NamedData NDName="pText" NDType="java.lang.String" NDValue="${bindings.ExecuteWithParams_pText}"/>
    </action>
    <list IterBinding="variables" id="pResponsible" DTSupportsMRU="true"
          StaticList="false" ListIter="ProgrammeManagerLOV1Iterator"
          NullValueId="${adfBundle['oracle.javatools.resourcebundle.SystemBundle']['NULL_VALUE_RESID']}"
          NullValueFlag="start">
      <AttrNames>
        <Item Value="ExecuteWithParams_pResponsible"/>
      </AttrNames>
      <ListAttrNames>
        <Item Value="PersId"/>
      </ListAttrNames>
      <ListDisplayAttrNames>
        <Item Value="Initials"/>
      </ListDisplayAttrNames>
    </list>
    <list IterBinding="variables" id="pProgramme" DTSupportsMRU="true"
          StaticList="false" ListIter="ProgrammeLOV1Iterator"
          NullValueId="${adfBundle['oracle.javatools.resourcebundle.SystemBundle']['NULL_VALUE_RESID']}"
          NullValueFlag="start">
      <AttrNames>
        <Item Value="ExecuteWithParams_pProgramme"/>
      </AttrNames>
      <ListAttrNames>
        <Item Value="ProgId"/>
      </ListAttrNames>
      <ListDisplayAttrNames>
        <Item Value="Name"/>
      </ListDisplayAttrNames>
    </list>
    <attributeValues IterBinding="variables" id="pText">
      <AttrNames>
        <Item Value="ExecuteWithParams_pText"/>
      </AttrNames>
    </attributeValues>
    <action id="Commit" RequiresUpdateModel="true" Action="commitTransaction" DataControl="XdmUC008ServiceDataControl"/>
    <action id="Rollback" RequiresUpdateModel="false" Action="rollbackTransaction"
            DataControl="XdmUC008ServiceDataControl"/>
  </bindings>
</pageDefinition>
